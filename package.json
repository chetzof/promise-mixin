{
  "name": "promise-mixin",
  "version": "1.0.0",
  "description": "oclif example Hello World CLI",
  "author": "chetzof @chetzof",
  "prettier": "chetzof-lint-config/prettier/.prettierrc.js",
  "homepage": "https://github.com/chetzof/promise-mixin",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/chetzof/promise-mixin.git"
  },
  "files": [
    "dist/*"
  ],
  "devDependencies": {
    "@types/node": "^18.15.5",
    "chetzof-lint-config": "^1.0.13",
    "debug": "^4.3.4",
    "eslint-define-config": "^1.17.0",
    "execa": "^5.1.1",
    "semantic-release": "^20.1.3",
    "tsc-alias": "^1.8.3",
    "tsconfig-paths": "^4.1.2",
    "type-fest": "^3.6.1",
    "typescript": "^5.0.2",
    "vite-tsconfig-paths": "^4.0.7",
    "vitest": "0.29.7"
  },
  "scripts": {
    "build": "tsc; tsc-alias",
    "watch": "tsc -w",
    "lint": "npm run lint:tsc; npm run lint:eslint",
    "fix": "npm run fix:prettier . && npm run fix:eslint .",
    "fix:prettier": "prettier --write --ignore-unknown",
    "fix:eslint": "NODE_ENV=production  eslint --fix",
    "lint:eslint": "NODE_ENV=production  eslint .",
    "lint:tsc": "tsc --noEmit",
    "prepack": "npm run build",
    "test": "vitest",
    "preparee": "husky install",
    "semantic-release": "semantic-release",
    "lint-staged:test": "vitest related --run"
  },
  "engines": {
    "node": ">=16"
  },
  "bugs": "https://github.com/chetzof/linktink/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "release": {
    "branches": [
      "master"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
